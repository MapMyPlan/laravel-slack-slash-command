{
    "name": "mapmyplan/laravel-slack-slash-command-slash-command",
    "description": "Make a Laravel app respond to a slash command from Slack",
    "keywords": [
        "spatie",
        "laravel-slack"
    ],
    "homepage": "https://github.com/MapMyPlan/laravel-slack-slash-command",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Rubens Peculis",
            "email": "rubens@mapmyplan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.2",
        "illuminate/support": "~5.8.0|^6.0",
        "illuminate/routing": "~5.8.0|^6.0",
        "illuminate/config": "~5.8.0|^6.0",
        "illuminate/queue": "~5.8.0|^6.0",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "laravel/helpers": "^1.0"

    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench":  "~3.8.0|^4.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "MapMyPlan\\SlashCommand\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MapMyPlan\\SlashCommand\\SlashCommandServiceProvider"
            ]
        }
    }
}
